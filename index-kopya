client.on('message', async message => {
if (message.content === 'fakeayrÄ±l') { // Buraya ne yazarsanÄ±z yazdÄ±ÄŸÄ±nÄ±z ÅŸeye gÃ¶re Ã§alÄ±ÅŸÄ±r
